<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>emails example</title>
  <link rel="stylesheet" type="text/css" href="/stylesheets/iview.css">
  <script type="text/javascript" src="/javascripts/vue.min.js"></script>
  <script type="text/javascript" src="/javascripts/iview.min.js"></script>
  <script type="text/javascript" src="/javascripts/axios.min.js"></script>
</head>
<body>
  <div id="app">
    <i-table :columns="columns" :data="emails">
      <template slot-scope="{ row }" slot="from">
        {{ getContactString(row.fromAddress, row.fromName) }}
      </template>
      <template slot-scope="{ row }" slot="to">
        {{ getContactString(row.toAddress, row.toName) }}
      </template>
      <template slot-scope="{ row }" slot="createdAt">
        {{ new Date(row.createdAt) }}
      </template>
    </i-table>
  </div>
  <script type="text/javascript" src="/javascripts/main.js"></script>
</body>
</html>
